{
    "name": "hubotActivity",
    "version": "0.1",
    "classes": [
        {
            "class": "ibmcloud.activity",
            "description": "Show recent activity",
            "emittarget": "ibmcloud.activity",
            "confirmprompt": "Would you like to see the activity for the ${timeframe} (yes or no)?",
            "texts": [
                "Show me the activity today.",
                "What have I done this week?",
                "What work have I done recently?"
            ],
			"parameters": [
				{
					"name": "timeframe",
					"type": "keyword",
					"prompt": "Enter desired timeframe (today | week)",
					"values": ["today", "week"]
				}
			]
        }
    ]
}
